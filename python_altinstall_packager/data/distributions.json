{
  "el9": {
    "dockerfile": {
      "from_image": "quay.io/centos/centos:stream9",
      "image_preparation": [
        "dnf -y upgrade",
        "dnf -y install rpmdevtools",
        "dnf config-manager --enable crb"
      ],
      "sources_preparation": [
        "for spec_file in *.spec; do dnf builddep -y \"$spec_file\"; done"
      ]
    },
    "packaging_tool": "rpmbuild",
    "requires": [
      "bzip2-devel",
      "gcc",
      "gcc-c++",
      "gdbm-devel",
      "libffi-devel",
      "libnsl2-devel",
      "libuuid-devel",
      "openssl-devel",
      "readline-devel",
      "sqlite-devel",
      "tk-devel"
    ]
  }
}